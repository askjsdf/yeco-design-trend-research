{
  "rewrites": [
    {
      "source": "/",
      "destination": "/pages/home/index.html"
    },
    {
      "source": "/home",
      "destination": "/pages/home/index.html"
    },
    {
      "source": "/trends",
      "destination": "/pages/trends/index.html"
    },
    {
      "source": "/reports/2024-10-middleeast",
      "destination": "/pages/reports/2024-10-middleeast/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/home.html",
      "destination": "/home",
      "permanent": true
    },
    {
      "source": "/trends.html",
      "destination": "/trends",
      "permanent": true
    },
    {
      "source": "/pages/home",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/pages/trends",
      "destination": "/trends",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)\\.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/javascript; charset=utf-8"
        }
      ]
    },
    {
      "source": "/(.*)\\.css",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css; charset=utf-8"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/shared/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)\\.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "trailingSlash": false,
  "cleanUrls": true
}
